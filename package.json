{
  "name": "hijacker",
  "version": "0.0.0-development",
  "description": "Frontend development tool for intercepting and mocking api requests",
  "author": "Travis Weidenbenner <travis@thetravisw.com>",
  "license": "MIT",
  "repository": "https://github.com/travis-w/hijacker",
  "homepage": "http://hijacker.io",
  "bin": "bin/hijacker.js",
  "main": "lib/hijacker.js",
  "dependencies": {
    "body-parser": "^1.17.2",
    "commander": "^2.11.0",
    "express": "^4.15.3",
    "express-xml-bodyparser": "^0.3.0",
    "node-fetch": "^2.2.0",
    "socket.io": "^2.0.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "codemirror": "^5.43.0",
    "commitizen": "^3.0.5",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.5",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-standard": "^2.1.6",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "lodash": "^4.17.10",
    "nock": "^10.0.6",
    "node-sass": "^4.9.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^7.0.1",
    "semantic-release": "^16.0.0-beta.18",
    "socket.io-client": "^2.0.3",
    "vue": "^2.5.16",
    "vue-loader": "^15.0.10",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "commit": "git-cz",
    "coveralls": "coveralls < test/coverage/lcov.info",
    "dev:back": "nodemon bin/hijacker.js",
    "dev:front": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "lint": "npm run lint:front && npm run lint:back",
    "lint:front": "eslint --ext .js --ext .vue src/",
    "lint:back": "eslint --ext .js lib/backend/ test/",
    "semantic-release": "semantic-release",
    "start": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "test": "jest --coverage"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*"
    ]
  }
}
