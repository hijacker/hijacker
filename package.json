{
  "name": "hijacker",
  "version": "0.0.0-development",
  "description": "Frontend development tool for intercepting and mocking api requests",
  "author": "Travis Weidenbenner <travis@thetravisw.com>",
  "license": "MIT",
  "repository": "travis-w/hijacker",
  "homepage": "http://hijacker.io",
  "bin": "bin/hijacker.js",
  "main": "lib/hijacker.js",
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "commander": "^2.11.0",
    "express": "^4.15.3",
    "socket.io": "^2.0.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-standard": "^1.4.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^21.1.0",
    "jsoneditor": "^5.11.0",
    "lodash.isequal": "^4.5.0",
    "nock": "^9.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.6",
    "semantic-release": "^17.2.3",
    "socket.io-client": "^2.0.3",
    "svg-url-loader": "^2.1.1",
    "vue": "^2.4.4",
    "vue-loader": "^13.0.5",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.4",
    "vuex": "^2.3.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "scripts": {
    "commit": "gz-commit",
    "coveralls": "coveralls < test/coverage/lcov.info",
    "lint": "npm run lint:frontend && npm run lint:server",
    "lint:frontend": "eslint --ext .js --ext .vue src/",
    "lint:server": "eslint --ext .js lib/backend/ test",
    "start": "nodemon bin/hijacker.js",
    "test": "jest --coverage",
    "semantic-release": "semantic-release",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master"
  }
}
