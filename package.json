{
  "name": "hijacker",
  "version": "0.0.0-development",
  "description": "Frontend development tool for intercepting and mocking api requests",
  "author": "Travis Weidenbenner <travis@travisw.dev>",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "npm run lint:front && npm run lint:back",
    "commit": "git-cz",
    "coveralls": "coveralls < test/coverage/lcov.info",
    "dev:back": "nodemon bin/hijacker.js",
    "dev:front": "vue-cli-service serve",
    "lint:back": "eslint --ext .js lib/ test/",
    "lint:front": "eslint --ext .js --ext .vue src/",
    "semantic-release": "semantic-release",
    "start": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test": "jest --coverage"
  },
  "main": "lib/hijacker.js",
  "files": [
    "bin/",
    "lib/"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "commander": "^4.1.1",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "luxon": "^1.22.0",
    "node-fetch": "^2.6.0",
    "route-matcher": "^0.1.0",
    "socket.io": "^2.3.0",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addon-notes": "^5.3.14",
    "@storybook/vue": "^5.3.14",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-router": "^4.2.3",
    "@vue/cli-plugin-vuex": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "codemirror": "^5.52.0",
    "commitizen": "^4.0.3",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.4",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-standard": "^3.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.2.1",
    "jest": "^25.1.0",
    "nock": "^12.0.1",
    "nodemon": "^2.0.2",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "semantic-release": "^17.0.4",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11",
    "vue-cli-plugin-storybook": "^1.2.1",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bin": "bin/hijacker.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "homepage": "http://hijacker.io",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*"
    ]
  },
  "repository": "https://github.com/travis-w/hijacker"
}
