{
  "name": "hijacker",
  "version": "0.0.0-development",
  "description": "Frontend development tool for intercepting and mocking api requests",
  "author": "Travis Weidenbenner <travis@travisw.dev>",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "npm run lint:front && npm run lint:back",
    "commit": "git-cz",
    "dev:back": "nodemon bin/hijacker.js",
    "dev:front": "vue-cli-service serve",
    "lint:back": "eslint --ext .js lib/ test/",
    "lint:front": "eslint --ext .js --ext .vue src/",
    "semantic-release": "semantic-release",
    "start": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test": "jest --coverage"
  },
  "main": "lib/hijacker.js",
  "files": [
    "bin/",
    "lib/"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "commander": "^7.0.0",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "got": "^11.8.1",
    "luxon": "^1.25.0",
    "route-matcher": "^0.1.0",
    "socket.io": "^3.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/vue": "^6.1.14",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-router": "^4.5.10",
    "@vue/cli-plugin-vuex": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "codemirror": "^5.59.2",
    "commitizen": "^4.2.3",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.18.0",
    "eslint-plugin-vue": "^7.5.0",
    "jest": "^26.6.3",
    "nock": "^13.0.6",
    "nodemon": "^2.0.7",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "semantic-release": "^17.3.6",
    "socket.io-client": "^3.1.0",
    "vue": "^2.6.12",
    "vue-cli-plugin-storybook": "^2.0.0",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bin": "bin/hijacker.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "homepage": "http://hijacker.io",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*"
    ]
  },
  "repository": "https://github.com/travis-w/hijacker"
}
