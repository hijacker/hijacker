{
  "name": "hijacker",
  "version": "0.0.0-development",
  "description": "Frontend development tool for intercepting and mocking api requests",
  "author": "Travis Weidenbenner <travis@thetravisw.com>",
  "license": "MIT",
  "repository": "travis-w/hijacker",
  "homepage": "http://hijacker.io",
  "bin": "bin/hijacker.js",
  "main": "lib/hijacker.js",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.17.2",
    "commander": "^2.11.0",
    "express": "^4.15.3",
    "express-xml-bodyparser": "^0.3.0",
    "node-fetch": "^2.2.0",
    "socket.io": "^2.0.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "commitizen": "^2.9.6",
    "concurrently": "^3.6.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-standard": "^1.4.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "lodash": "^4.17.10",
    "nock": "^9.1.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^7.0.1",
    "semantic-release": "^15.4.1",
    "socket.io-client": "^2.0.3",
    "vue": "^2.5.16",
    "vue-loader": "^15.0.10",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "commit": "git-cz",
    "coveralls": "coveralls < test/coverage/lcov.info",
    "dev:back": "nodemon bin/hijacker.js",
    "dev:front": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "lint": "npm run lint:front && npm run lint:back",
    "lint:front": "eslint --ext .js --ext .vue src/",
    "lint:back": "eslint --ext .js lib/backend/ test/",
    "semantic-release": "semantic-release",
    "start": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "test": "jest --coverage"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master"
  }
}
