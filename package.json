{
  "name": "hijacker",
  "version": "0.0.0-development",
  "description": "Frontend development tool for intercepting and mocking api requests",
  "author": "Travis Weidenbenner <travis@thetravisw.com>",
  "license": "MIT",
  "repository": "https://github.com/travis-w/hijacker",
  "homepage": "http://hijacker.io",
  "bin": "bin/hijacker.js",
  "main": "lib/hijacker.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "commander": "^4.0.1",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "luxon": "^1.21.3",
    "node-fetch": "^2.6.0",
    "route-matcher": "^0.1.0",
    "socket.io": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "codemirror": "^5.52.0",
    "commitizen": "^4.0.3",
    "concurrently": "^5.0.2",
    "core-js": "^3.6.4",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-standard": "^3.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^24.9.0",
    "nock": "^11.7.1",
    "nodemon": "^2.0.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "semantic-release": "^16.0.0-beta.25",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "scripts": {
    "build": "vue-cli-service build",
    "commit": "git-cz",
    "coveralls": "coveralls < test/coverage/lcov.info",
    "dev:back": "nodemon bin/hijacker.js",
    "dev:front": "vue-cli-service serve",
    "lint": "npm run lint:front && npm run lint:back",
    "lint:front": "eslint --ext .js --ext .vue src/",
    "lint:back": "eslint --ext .js lib/ test/",
    "semantic-release": "semantic-release",
    "start": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "test": "jest --coverage"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
