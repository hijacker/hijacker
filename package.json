{
  "name": "hijacker",
  "version": "0.0.0-development",
  "description": "Frontend development tool for intercepting and mocking api requests",
  "author": "Travis Weidenbenner <travis@travisw.dev>",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "npm run lint:front && npm run lint:back",
    "commit": "git-cz",
    "dev": "node dev-server.js",
    "dev:back": "nodemon bin/hijacker.js",
    "dev:front": "vue-cli-service serve",
    "lint:back": "eslint --ext .js lib/ test/",
    "lint:front": "eslint --ext .js --ext .vue src/",
    "semantic-release": "semantic-release",
    "start": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "test": "jest --coverage"
  },
  "main": "output/hijacker.js",
  "files": [
    "bin/",
    "lib/"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "express": "^4.17.1",
    "express-xml-bodyparser": "^0.3.0",
    "got": "^11.8.2",
    "luxon": "^1.27.0",
    "route-matcher": "^0.1.0",
    "socket.io": "^4.1.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-xml-bodyparser": "^0.3.2",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.21.1",
    "commitizen": "^4.2.4",
    "concurrently": "^6.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.13.13",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "monaco-editor": "^0.25.1",
    "monaco-editor-webpack-plugin": "^4.0.0",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sass": "^1.35.1",
    "sass-loader": "^10.1.1",
    "semantic-release": "^17.4.4",
    "socket.io-client": "^4.1.2",
    "tiny-glob": "^0.2.9",
    "typescript": "^4.5.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bin": "bin/hijacker.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "homepage": "http://hijacker.io",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*"
    ]
  },
  "repository": "https://github.com/travis-w/hijacker"
}
