{
  "name": "hijacker",
  "version": "0.0.0-development",
  "description": "Frontend development tool for intercepting and mocking api requests",
  "author": "Travis Weidenbenner <travis@thetravisw.com>",
  "license": "MIT",
  "repository": "travis-w/hijacker",
  "homepage": "http://hijacker.io",
  "bin": "bin/hijacker.js",
  "main": "lib/hijacker.js",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.17.2",
    "commander": "^2.11.0",
    "express": "^4.15.3",
    "socket.io": "^2.0.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-standard": "^1.4.1",
    "jest": "^22.4.3",
    "nock": "^9.1.0",
    "nodemon": "^1.11.0",
    "semantic-release": "^15.4.1",
    "socket.io-client": "^2.0.3"
  },
  "scripts": {
    "commit": "git-cz",
    "coveralls": "coveralls < test/coverage/lcov.info",
    "dev:back": "nodemon bin/hijacker.js",
    "dev:front": "",
    "lint": "npm run lint:frontend && npm run lint:server",
    "lint:frontend": "",
    "lint:server": "eslint --ext .js lib/backend/ test",
    "semantic-release": "semantic-release",
    "start": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "test": "jest --coverage"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master"
  }
}
