  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "body-parser": "^1.20.2",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "express-xml-bodyparser": "^0.3.0",
    "got": "^13.0.0",
    "route-matcher": "^0.1.0",
    "uuid": "^9.0.1",
    "vite": "^4.5.0",
    "vite-node": "^0.34.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hijacker/eslint-config": "workspace:*",
    "@hijacker/tsconfig": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/express-xml-bodyparser": "^0.3.5",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-istanbul": "^0.34.6",
    "eslint": "^8.53.0",
    "nock": "^13.3.8",
