{
  "name": "@hijacker/plugin-frontend",
  "version": "1.0.0-beta.6",
  "type": "module",
  "main": "dist/index.js",
  "description": "Hijacker plugin that adds a web interface to add and modify rules.",
  "author": "Travis Weidenbenner <travis@weidenbenner.com>",
  "homepage": "http://hijacker.io",
  "license": "MIT",
  "repository": "https://github.com/travis-w/hijacker",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm build:back && pnpm build:front",
    "build:back": "tsc --project tsconfig.build.json",
    "build:front": "vite build",
    "dev": "concurrently \"pnpm dev:back\" \"pnpm dev:front\"",
    "dev:back": "tsc --project tsconfig.build.json --watch",
    "dev:front": "NODE_ENV=development vite build --watch --emptyOutDir",
    "lint": "eslint src/",
    "test": "vitest --coverage --config vitest.config.ts",
    "storybook": "storybook dev -p 6006 -c src/frontend/.storybook",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hijacker/core": "workspace:*",
    "@hijacker/eslint-config": "workspace:*",
    "@hijacker/tsconfig": "workspace:*",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "5.0.0-alpha.152",
    "@mui/material": "^5.14.17",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-istanbul": "^0.34.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "got": "^13.0.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.44.0",
    "nock": "^13.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "socket.io-client": "^4.7.2",
    "storybook": "^7.5.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@hijacker/core": "workspace:*"
  }
}
