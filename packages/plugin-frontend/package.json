{
  "name": "@hijacker/plugin-frontend",
  "version": "1.0.0-beta.4",
  "type": "module",
  "main": "dist/index.js",
  "description": "Hijacker plugin that adds a web interface to add and modify rules.",
  "author": "Travis Weidenbenner <travis@weidenbenner.com>",
  "homepage": "http://hijacker.io",
  "license": "MIT",
  "repository": "https://github.com/travis-w/hijacker",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm build:back && pnpm build:front",
    "build:back": "tsc",
    "build:front": "vite build",
    "dev": "concurrently \"pnpm dev:back\" \"pnpm dev:front\"",
    "dev:back": "tsc --watch",
    "dev:front": "NODE_ENV=development vite build --watch --emptyOutDir",
    "lint": "eslint src/",
    "test": "vitest --coverage --config vitest.config.ts"
  },
  "dependencies": {
    "express": "^4.18.1",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hijacker/core": "workspace:*",
    "@hijacker/eslint-config": "workspace:*",
    "@hijacker/tsconfig": "workspace:*",
    "@monaco-editor/react": "^4.4.6",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.12",
    "@types/express": "^4.17.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-istanbul": "^0.22.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.23.0",
    "got": "^12.3.1",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.34.1",
    "nock": "^13.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "rollup-plugin-visualizer": "^5.8.3",
    "socket.io-client": "^4.5.3",
    "typescript": "^4.6.4",
    "vite": "^3.2.2",
    "vitest": "^0.22.1"
  },
  "peerDependencies": {
    "@hijacker/core": "workspace:*"
  }
}