{
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"]
  },
  "globals": {
    "JSONEditor": true
  },
  "root": true
}
