{
  "plugins": ["html"],
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"]
  },
  "globals": {
    "JSONEditor": true
  },
  "root": true
}
